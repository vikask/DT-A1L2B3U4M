<% provide(:title, 'Home') %>
<% if signed_in? %>
    <p>Welcome</p>
<% else %>

      <div id="myCarousel" class="carousel slide">
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Carousel items -->
        <div class="carousel-inner">
          <div class="active item"><img alt="" src="assets/coursel/01.jpg"></div>
          <div class="item"><img alt="" src="assets/coursel/02.jpg"></div>
          <div class="item"><img alt="" src="assets/coursel/03.jpg"></div>
          <div class="item"><img alt="" src="assets/coursel/04.jpg"></div>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
      </div>
      <%= link_to "Sign Up now!", new_user_registration_path, :class => "btn btn-large btn-primary btn-radius" %>

<% end %>


